s1 = 8
s2 = 3
p1 = 0
p2 = 0
a1 = 6
a2 = 15
b1 = 0
b2 = 0
for x in range(1,1000):
    b1= s1+a1
    if(b1%10 == 0):
        b1 = 10
    else:
        b1=b1%10
    p1+=b1
    a1+=18
    s1=b1
    print(x)
    print(p1)
    if (p1 >= 1000):
        break

for x in range(1,1000):
    b2= s2+a2
    if(b2%10 == 0):
        b2 = 10
    else:
        b2=b2%10
    p2+=b2
    a2+=18
    s2=b2
    print(x)
    print(p2)
    if (p2 >= 1000):
        break

